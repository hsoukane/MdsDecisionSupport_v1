package cdss.myproject;
import org.kie.api.runtime.KieSession;
import cdss.VitalParameterMeasurement;

rule "measurementTrigger"
ruleflow-group"TriggersRules"
no-loop true
when
kieSession: KieSession()
measurement: cdss.VitalParameterMeasurement(name=="Blood glucose test" && nurseAgreement==true)
then
kieSession.signalEvent("glucose test", null);
end


