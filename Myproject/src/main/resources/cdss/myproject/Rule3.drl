package cdss.myproject;
import cdss.PatientRecord;
import cdss.MedicalExamination;
import cdss.Assignement;


rule"Clinical examinations"
ruleflow-group"ClinicalExamsRules"
no-loop true
salience 3
when
patient: cdss.PatientRecord()
triage:cdss.Assignement( service=="Endocrinology and diabetology")
me:cdss.MedicalExamination(this==null )
then

me.setName("Diabetic examination");
patient.getMedicalExaminations().add(0,me);
System.out.println(me.getName()+"must be done");

end

